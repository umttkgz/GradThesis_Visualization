   "source": [
    "import pandas as pd\n",
    "\n",
    "\n",
    "df = pd.read_csv('updated.csv')\n",
    "\n",
    "# Sum up the total number of diseases for every week\n",
    "df_sum = df.groupby(['Month', 'Region', 'Disease'], as_index=False)['Total'].sum()\n",
    "\n",
    "\n",
    "\n",
    "df_sum.to_csv('modified_dataset.csv', index=False)\n"
   ]
  }
